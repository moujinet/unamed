{
  "name": "@mouji/unamed",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.7.5",
  "license": "MIT",
  "homepage": "https://github.com/moujinet/unamed",
  "main": "./nuxt.config.ts",
  "scripts": {
    "start": "node .output/server/index.mjs",
    "build": "nuxt build",
    "dev": "nr prepare:env && nr prepare:base && nuxt dev --port 8553",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "commit": "czg",
    "typecheck": "stale-dep && nuxi typecheck",
    "prepare": "nr prepare:env && nr prepare:base && nr prepare:prisma",
    "prepare:env": "ignore-dependency-scripts \"tsx scripts/build-env.ts\"",
    "prepare:base": "ignore-dependency-scripts \"tsx scripts/prepare.ts\"",
    "prepare:prisma": "npx prisma db push",
    "postinstall": "stale-dep -u && simple-git-hooks && nuxt prepare",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@morev/vue-transitions": "^2.3.6",
    "@nuxt/image": "1.0.0-rc.1",
    "@nuxtjs/color-mode": "^3.3.0",
    "@prisma/client": "^5.3.0",
    "@unocss/nuxt": "^0.55.7",
    "@unocss/reset": "^0.55.7",
    "@vueuse/core": "^10.4.1",
    "@vueuse/nuxt": "^10.4.1",
    "dayjs": "^1.11.9",
    "defu": "^6.1.2",
    "floating-vue": "2.0.0-beta.24",
    "nuxt-icon": "^0.5.0",
    "prisma": "^5.3.0",
    "stale-dep": "^0.7.0",
    "unocss": "^0.55.7",
    "vue-i18n": "^9.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.3",
    "@antfu/ni": "^0.21.8",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify-json/ph": "^1.1.6",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@nuxt/devtools": "latest",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.6.0",
    "@unocss/eslint-config": "^0.55.7",
    "@unocss/preset-rem-to-px": "^0.55.7",
    "cheerio": "1.0.0-rc.12",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "eslint": "^8.49.0",
    "fs-extra": "^11.1.1",
    "happy-dom": "^11.0.5",
    "ignore-dependency-scripts": "^1.0.1",
    "lint-staged": "^14.0.1",
    "magic-string": "^0.30.3",
    "nuxt": "^3.7.2",
    "postcss": "^8.4.29",
    "postcss-nested": "^6.0.1",
    "simple-git": "^3.19.1",
    "simple-git-hooks": "^2.9.0",
    "std-env": "^3.4.3",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2",
    "vitepress": "^1.0.0-rc.13",
    "vue-tsc": "^1.8.11"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
